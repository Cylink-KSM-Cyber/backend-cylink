{
  "name": "test-cylink",
  "version": "1.0.0",
  "description": "Cylink URL Shortener and QR Generator",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "db:migrate": "ts-node -r tsconfig-paths/register src/database/migrations.ts",
    "db:seed": "ts-node -r tsconfig-paths/register src/database/seed.ts",
    "db:seed:admin": "ts-node -r tsconfig-paths/register src/database/seed.ts admin",
    "db:seed:urls": "ts-node -r tsconfig-paths/register src/database/seed.ts urls",
    "db:seed:qrcodes": "ts-node -r tsconfig-paths/register src/database/seed.ts qrcodes",
    "db:seed:clicks": "ts-node -r tsconfig-paths/register src/database/seed.ts clicks",
    "db:seed:all": "npm run db:seed:admin && npm run db:seed:urls && npm run db:seed:qrcodes && npm run db:seed:clicks",
    "lint": "eslint \"**/*.{js,ts}\"",
    "lint:fix": "eslint \"**/*.{js,ts}\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haikalrafifas/test-cylink.git"
  },
  "keywords": [],
  "author": "ksm-cyber",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haikalrafifas/test-cylink/issues"
  },
  "homepage": "https://github.com/haikalrafifas/test-cylink#readme",
  "dependencies": {
    "@types/cors": "^2.8.17",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "mailtrap": "^4.0.0",
    "module-alias": "^2.2.3",
    "nodemailer": "^6.10.0",
    "pg": "^8.14.1",
    "qrcode": "^1.5.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^22.14.0",
    "@types/pg": "^8.11.11",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "dotenv": "^16.4.7",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-promise": "^7.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}

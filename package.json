{
  "name": "backend-cylink",
  "version": "1.3.0",
  "description": "Cylink URL Shortener and QR Generator",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start": "node dist/index.js",
    "build": "tsc && npm run sentry:sourcemaps",
    "dev": "concurrently -k -n api,watch -c green,cyan \"nodemon\" \"npm run migration:watch\"",
    "migration:new": "ts-node -r tsconfig-paths/register src/database/scripts/generateMigration.ts",
    "migration:watch": "ts-node -r tsconfig-paths/register src/database/scripts/watchMigrations.ts",
    "db": "npx knex migrate:latest --knexfile src/config/knex.ts",
    "db:migrate": "npx knex migrate:latest --knexfile src/config/knex.ts",
    "db:migrate:rollback": "npx knex migrate:rollback --knexfile src/config/knex.ts",
    "db:migrate:status": "npx knex migrate:status --knexfile src/config/knex.ts",
    "db:migrate:make": "npx knex migrate:make --knexfile src/config/knex.ts",
    "db:seed": "ts-node -r tsconfig-paths/register src/database/seed.ts",
    "db:seed:admin": "ts-node -r tsconfig-paths/register src/database/seed.ts admin",
    "db:seed:urls": "ts-node -r tsconfig-paths/register src/database/seed.ts urls",
    "db:seed:qrcodes": "ts-node -r tsconfig-paths/register src/database/seed.ts qrcodes",
    "db:seed:clicks": "ts-node -r tsconfig-paths/register src/database/seed.ts clicks",
    "db:seed:impressions": "ts-node -r tsconfig-paths/register src/database/seed.ts impressions",
    "db:seed:goals": "ts-node -r tsconfig-paths/register src/database/seed.ts goals",
    "db:seed:conversions": "ts-node -r tsconfig-paths/register src/database/seed.ts conversions",
    "db:seed:all": "npm run db:seed:admin && npm run db:seed:urls && npm run db:seed:qrcodes && npm run db:seed:clicks && npm run db:seed:impressions && npm run db:seed:goals && npm run db:seed:conversions",
    "lint": "eslint \"**/*.{js,ts}\"",
    "lint:fix": "eslint \"**/*.{js,ts}\" --fix",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org nugraha-adhitama --project api-cylink ./dist && sentry-cli sourcemaps upload --org nugraha-adhitama --project api-cylink ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haikalrafifas/test-cylink.git"
  },
  "keywords": [],
  "author": "ksm-cyber",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haikalrafifas/test-cylink/issues"
  },
  "homepage": "https://github.com/haikalrafifas/test-cylink#readme",
  "dependencies": {
    "@faker-js/faker": "^9.7.0",
    "@sentry/cli": "^2.47.1",
    "@sentry/node": "^9.47.1",
    "@sentry/profiling-node": "^9.38.0",
    "@types/cors": "^2.8.17",
    "@types/winston": "^2.4.4",
    "bcrypt": "^5.1.1",
    "concurrently": "^8.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "express-basic-auth": "^1.2.1",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "mailtrap": "^3.1.1",
    "module-alias": "^2.2.3",
    "nodemailer": "^7.0.10",
    "pg": "^8.14.1",
    "qrcode": "^1.5.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/ansi-colors": "^3.0.1",
    "@types/express": "^5.0.1",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.14",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^22.14.0",
    "@types/pg": "^8.11.11",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "ansi-colors": "^4.1.3",
    "chokidar": "^3.6.0",
    "date-fns": "^3.6.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-promise": "^7.2.1",
    "inquirer": "^9.2.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
